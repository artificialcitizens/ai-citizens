{
  "name": "@ai-citizens/ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--import=tsimp"
    ]
  },
  "scripts": {
